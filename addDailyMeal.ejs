<h3>Add Daily Meal</h3>
    <form method="POST">
        <div class="form-group">
            <label for="breakfast">Breakfast</label>
            <input type="text" name="breakfast">
        </div>
		<div class="form-group">
            <label for="morningTea">Morning tea</label>
			<input type="text" name="morningTea">
        </div>
		<div class="form-group">
            <label for="lunch">Lunch</label>
            <input type="text" name="lunch">
        </div>
        <div class="form-group">
            <label for="afternoonTea">Afternoon Tea</label>
            <input type="text" name="afternoonTea">
        </div>
        <div class="form-group">
            <label for="childID">Child ID</label>
            <input type="text" name="childID">
        </div>
        <button type="submit" class="btn btn-primary">Add Daily Meal</button>
    </form>
